{
  "name": "my-material-gatsby-site",
  "description": "My Material Gatsby site",
  "license": "Unlicense",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build --prefix-paths",
